<section class="new-banner-wrapper">
  <div class="banner-slider" id="banner-slider">
    {% assign counter = 0   %}
    {% for block in section.blocks %}
      {% assign counter = counter | plus: 1 %}
      <div class="banner-sld-items">
        <img class="img-fluid" src="{{ block.settings.image_banner | img_url: 'master' }}" alt="">
        
          <div class="new-banner-content">
              <div class="banner-inner-content">
                <h3>{{ block.settings.banner_hedding }}</h3>
                <h4>{{ block.settings.banner_sub_hedding }}</h4>
                <p>{{ block.settings.banner_child_hedding }}</p>

                {% if counter != 2 %}                  
                  <a class="btn btn-gradient" href="/pages/shop/">Shop Now <i class="fa-solid fa-arrow-right"></i></a>
                {% else %}
                  <div class="bnr-form">
                    {% comment %} <input class="form-control" type="text" placeholder="Enter Your e-mail">
                    <input class="btn btn-gradient" type="submit" value="Submit"> {% endcomment %}

                    {% form 'customer'
                      , class: 'newsletter-form' %}
                      <input
                        type="hidden"
                        name="contact[tags]"
                        value="newsletter">                    
                        
                          <input
                            class="form-control"
                            type="text"
                            value="{{ form.email }}"
                            name="contact[email]"
                            placeholder="Enter Your e-mail"
                            required>                       
                        
                          <input
                            class="btn btn-gradient"
                            type="submit"
                            name="commit"
                            value="Submit">
                        
                        {%- if form.errors -%}
                          <small class="newsletter-form__message form__message" id="Newsletter-error--{{ section.id }}">
                            {%- render 'icon-error' -%}
                            {{- form.errors.translated_fields.email | capitalize }}
                            {{ form.errors.messages.email -}}
                          </small>
                        {%- endif -%}
                     
                      {%- if form.posted_successfully? -%}
                        <h3
                          class="newsletter-form__message newsletter-form__message--success form__message"
                          id="Newsletter-success--{{ section.id }}"
                          tabindex="-1"
                          autofocus>
                          {% render 'icon-success' -%}
                          {{- 'newsletter.success' | t }}
                        </h3>
                      {%- endif -%}
                    {% endform %}
                  </div>
                {% endif %}

              </div>
             
          </div>
      </div>     
      
    {% endfor %}
  </div>
</section>


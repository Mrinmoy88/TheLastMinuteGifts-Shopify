                          <del>{{ product.compare_at_price | money }}</del>
                        {% endif %}
                      </div>
                      {% comment %}
                        <div class="rating-wrap">
                          {% comment %} <img class="img-fluid" src="images/rating-img.png" alt=""> {% endcomment %}
                          <img class="img-fluid" src="{{ 'rating-img.png' | asset_url }}" alt="">
